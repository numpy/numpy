 *         datetime, timedelta#
 */
static Bool
@fname@_nonzero (char *ip, PyArrayObject *ap)
{
    if (ap == NULL || PyArray_ISBEHAVED_RO(ap)) {
        @type@ *ptmp = (@type@ *)ip;
        return (Bool) (*ptmp != 0);
    }
    else {
        /*
         * don't worry about swap, since we are just testing
         * whether or not equal to 0
         */
        @type@ tmp;
        memcpy(&tmp, ip, sizeof(@type@));
        return (Bool) (tmp != 0);
    }
}
