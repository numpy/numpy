static npy_hash_t
@lname@_arrtype_hash(PyObject *obj)
{
    PyArray_DatetimeMetaData *meta;
    npy_@lname@ val = PyArrayScalar_VAL(obj, @name@);

    if (val == NPY_DATETIME_NAT) {
        /* Use identity, similar to NaN */
        return PyBaseObject_Type.tp_hash(obj);
